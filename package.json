{
    "name": "@gogocat/data-bind",
    "version": "1.12.0",
    "description": "dataBind is a small and fast MVVM tool for building front-end web",
    "main": "dist/js/dataBind.js",
    "types": "dist/js/types/index.d.ts",
    "type": "module",
    "scripts": {
        "test": "vitest run",
        "test:watch": "vitest",
        "test:coverage": "vitest run --coverage",
        "build": "npm run lint && rollup -c",
        "watch": "rollup -c -w",
        "typecheck": "tsc --noEmit",
        "lint": "eslint src test --fix"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/gogocat/dataBind.git"
    },
    "keywords": [
        "frontend",
        "data bind",
        "javascript"
    ],
    "author": "Adam Chow",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/gogocat/dataBind/issues"
    },
    "homepage": "https://gogocat.github.io/dataBind/",
    "devDependencies": {
        "@babel/cli": "^7.26.4",
        "@babel/core": "^7.26.0",
        "@babel/plugin-transform-object-assign": "^7.25.9",
        "@babel/preset-env": "^7.26.0",
        "@eslint/js": "^9.17.0",
        "@rollup/plugin-babel": "^6.0.4",
        "@rollup/plugin-commonjs": "^28.0.1",
        "@rollup/plugin-replace": "^6.0.2",
        "@rollup/plugin-terser": "^0.4.4",
        "@rollup/plugin-typescript": "^12.1.4",
        "@testing-library/dom": "^10.4.0",
        "@types/node": "^24.7.2",
        "@vitest/coverage-v8": "^3.2.4",
        "eslint": "^9.17.0",
        "globals": "^15.14.0",
        "jsdom": "^25.0.1",
        "rollup": "^4.30.1",
        "rollup-plugin-banner2": "^1.3.0",
        "tslib": "^2.8.1",
        "typescript": "^5.9.3",
        "typescript-eslint": "^8.46.0",
        "vitest": "^3.2.4"
    }
}
